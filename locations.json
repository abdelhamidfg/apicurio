{
    "openapi": "3.0.1",
    "info": {
        "title": "LocationsV1",
        "version": "1.0.0",
        "description": "Locations API"
    },
    "servers": [
        {
            "url": "/"
        }
    ],
    "paths": {
        "/locations": {
            "get": {
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/location"
                                    }
                                },
                                "example": [
                                    {
                                        "id": 22,
                                        "name": "some text",
                                        "type": "some text",
                                        "status": 27,
                                        "location": {
                                            "lat": "some text",
                                            "lng": "some text"
                                        }
                                    },
                                    {
                                        "id": 44,
                                        "name": "some text",
                                        "type": "some text",
                                        "status": 63,
                                        "location": {
                                            "lat": "some text",
                                            "lng": "some text"
                                        }
                                    }
                                ]
                            }
                        },
                        "description": "Successful response"
                    }
                }
            },
            "post": {
                "responses": {
                    "201": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/location"
                                }
                            }
                        },
                        "description": "Successfully created"
                    }
                }
            }
        },
        "/locations/{id}": {
            "get": {
                "parameters": [
                    {
                        "examples": {
                            "exam1": {
                                "value": "57"
                            }
                        },
                        "name": "id",
                        "schema": {
                            "format": "int32",
                            "type": "integer"
                        },
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/location"
                                },
                                "example": {
                                    "id": 39,
                                    "name": "some text",
                                    "type": "some text",
                                    "status": 69,
                                    "location": {
                                        "lat": "some text",
                                        "lng": "some text"
                                    }
                                }
                            }
                        },
                        "description": "Successful response"
                    }
                }
            }
        },
        "/location/phone/{id}": {
            "get": {
                "parameters": [
                    {
                        "examples": {
                            "exam1": {
                                "value": "50"
                            }
                        },
                        "name": "id",
                        "schema": {
                            "format": "int32",
                            "type": "integer"
                        },
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/location"
                                },
                                "example": {
                                    "id": 6,
                                    "name": "some text",
                                    "type": "some text",
                                    "status": 27,
                                    "location": {
                                        "lat": "some text",
                                        "lng": "some text"
                                    }
                                }
                            }
                        },
                        "description": "Successful response"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "location": {
                "title": "Root Type for location",
                "description": "The root of the location type's schema.",
                "type": "object",
                "properties": {
                    "id": {
                        "format": "int32",
                        "type": "integer"
                    },
                    "name": {
                        "type": "string"
                    },
                    "type": {
                        "type": "string"
                    },
                    "status": {
                        "format": "int32",
                        "type": "integer"
                    },
                    "location": {
                        "type": "object",
                        "properties": {
                            "lat": {
                                "type": "string"
                            },
                            "lng": {
                                "type": "string"
                            }
                        }
                    }
                },
                "example": "{\n  \"id\": 1,\n  \"name\": \"Kamarhati\",\n  \"type\": \"Regional Branch\",\n  \"status\": \"1\",\n  \"location\": {\n    \"lat\": \"-28.32555\",\n    \"lng\": \"-5.91531\"\n  }\n}"
            }
        }
    }
}